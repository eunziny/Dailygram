		MEMBER
		VALUES (#{id}, #{pwd}, #{name}, #{birthday}, #{email}, #{address}
		, #{gender}, #{joindate}, #{useyn}, #{profile_img}, #{publicyn}, #{intro})
	</insert> -->
	
</mapper>